@model MadamRozikaGlobal.Dtos.ViewDtos.ContentBase
<h4 class="box_header">Recommended PArtial</h4>
<ul class="blog small_margin clearfix">
    @{
        foreach (var reco in Model.Recommendeds.Take(3))
        {
            <li class="post">
                <a href="post_gallery.html" title="@reco.Title">
                    <span class="icon gallery"></span>
                    <img src='@reco.ImageUrl' alt='@reco.Title'>
                </a>
                <div class="post_content">
                    <h5>
                        <a href="post_gallery.html" title="@reco.Title">@reco.Title</a>
                    </h5>
                    <ul class="post_details simple">
                        <li class="category">
                            <a href="category_health.html" title="@reco.Category">@reco.Category</a>
                        </li>
                        <li class="date">
                            10:11 PM, Feb 02
                        </li>
                    </ul>
                </div>
            </li>
        }
    }

</ul>